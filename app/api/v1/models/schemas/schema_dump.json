        },
        "Platform": {
            "required": [
                "id",
                "name"
            ],
            "properties": {
                "id": {
                    "type": "integer",
                    "title": "Platform ID",
                    "description": "The unique identifier of a the platform",
                    "example": 1234
                },
                "name": {
                    "type": "string",
                    "title": "Name",
                    "description": "The name of the platform",
                    "example": "Digital Cloud"
                }
            },
            "type": "object"
        },
        "Target user": {
            "required": [
                "description",
                "id",
                "name"
            ],
            "properties": {
                "id": {
                    "type": "integer",
                    "title": "Target user",
                    "description": "The unique identifier of a the target user",
                    "example": 1234
                },
                "name": {
                    "type": "string",
                    "title": "Name",
                    "description": "The name of the target user",
                    "example": "Business"
                },
                "description": {
                    "type": "string",
                    "title": "Description",
                    "description": "Description of the target user",
                    "example": "The user of the service is a company"
                }
            },
            "type": "object"
        },
        "Access mode": {
            "required": [
                "description",
                "id",
                "name"
            ],
            "properties": {
                "id": {
                    "type": "integer",
                    "title": "Access mode",
                    "description": "The unique identifier of a the access mode",
                    "example": 1234
                },
                "name": {
                    "type": "string",
                    "title": "Name",
                    "description": "The name of the access mode",
                    "example": "open access"
                },
                "description": {
                    "type": "string",
                    "title": "Description",
                    "description": "Description of the access mode",
                    "example": "The access is open for everyone"
                }
            },
            "type": "object"
        },
        "Access type": {
            "required": [
                "description",
                "id",
                "name"
            ],
            "properties": {
                "id": {
                    "type": "integer",
                    "title": "Access type",
                    "description": "The unique identifier of a the access type",
                    "example": 1234
                },
                "name": {
                    "type": "string",
                    "title": "Name",
                    "description": "The name of the access type",
                    "example": "remote access"
                },
                "description": {
                    "type": "string",
                    "title": "Description",
                    "description": "Description of the access type",
                    "example": "Access can be remote"
                }
            },
            "type": "object"
        },
