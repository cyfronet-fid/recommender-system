        }
    },
    "responses": {
        "ParseError": {
            "description": "When a mask can't be parsed"
        },
        "MaskError": {
            "description": "When any error occurs on mask"
        },
        "InvalidRecommendationPanelVersionError": {},
        "InvalidRecommendationPanelLocationError": {}
    },
    "host": "localhost:5000"
}