        },
        "Provider": {
            "required": [
                "id",
                "name"
            ],
            "properties": {
                "id": {
                    "type": "integer",
                    "title": "Provider ID",
                    "description": "The unique identifier of a the provider",
                    "example": 1234
                },
                "name": {
                    "type": "string",
                    "title": "Name",
                    "description": "The name of the provider",
                    "example": "Cyfronet"
                }
            },
            "type": "object"
        },
        "Scientific domain": {
            "required": [
                "id",
                "name"
            ],
            "properties": {
                "id": {
                    "type": "integer",
                    "title": "Scientific domain ID",
                    "description": "The unique identifier of a the scientific domain",
                    "example": 1234
                },
                "name": {
                    "type": "string",
                    "title": "Name",
                    "description": "The name of the scientific domain",
                    "example": "Natural Sciences"
                }
            },
