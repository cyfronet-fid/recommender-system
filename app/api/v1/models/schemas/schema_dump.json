{
    "swagger": "2.0",
    "basePath": "/api/v1",
    "paths": {
        "/database_dumps": {
            "post": {
                "responses": {
                    "204": {
                        "description": "Database dump successfully sent"
                    }
                },
                "operationId": "post_database_dump",
                "parameters": [
                    {
                        "name": "payload",
                        "required": true,
                        "in": "body",
                        "schema": {
                            "$ref": "#/definitions/Database%20dump"
                        }
                    }
                ],
                "tags": [
                    "database_dumps"
                ]
            }
        },
        "/recommendations": {
            "post": {
                "responses": {
                    "200": {
                        "description": "Successfully fetched recommendations",
                        "schema": {
                            "type": "array",
                            "title": "Recommended services list",
                            "description": "List of the recommended services' IDs",
                            "example": [
                                1234,
                                2345,
                                3456
                            ],
                            "items": {
                                "type": "integer",
                                "title": "Service ID",
                                "description": "The unique identifier of a service"
                            }
                        }
                    }
                },
                "summary": "Returns list of ids of recommended scientific services",
                "operationId": "post_recommendation",
                "parameters": [
                    {
                        "name": "payload",
                        "required": true,
                        "in": "body",
                        "schema": {
                            "$ref": "#/definitions/Recommendation%20context"
                        }
                    },
                    {
                        "name": "location",
                        "in": "query",
                        "type": "string",
                        "required": true,
                        "description": "An unambiguous identifier of the recommendation panel location on the page",
                        "default": "services_list"
                    },
                    {
                        "name": "version",
                        "in": "query",
                        "type": "string",
                        "required": true,
                        "description": "An unambiguous identifier of the version of the recommendation panel",
                        "default": "v1"
                    }
                ],
                "tags": [
                    "recommendations"
                ]
            }
        },
        "/user_actions": {
            "post": {
                "responses": {
                    "204": {
                        "description": "User action successfully sent"
                    }
                },
                "operationId": "post_user_action",
                "parameters": [
                    {
                        "name": "payload",
                        "required": true,
                        "in": "body",
                        "schema": {
                            "$ref": "#/definitions/User%20Action"
                        }
                    }
                ],
                "tags": [
                    "user_actions"
                ]
            }
        }
    },
    "info": {
        "title": "Recommender system API",
        "version": "1.0",
        "description": "Recommender System API for getting recommendations, sending user     actions, sending Marketplace database dumps and triggering recommender system     offline learning."
    },
    "produces": [
        "application/json"
    ],
    "consumes": [
        "application/json"
    ],
    "tags": [
        {
            "name": "recommendations",
            "description": "Endpoint used for getting recommendations"
        },
        {
            "name": "user_actions",
            "description": "Endpoint used for sending user actions"
        },
        {
            "name": "database_dumps",
            "description": "Endpoint used for sending a database dump"
        }
    ],
    "definitions": {
        "Recommendation context": {
            "required": [
                "recommendation_page_visit_id",
                "user_id"
            ],
            "properties": {
                "user_id": {
                    "type": "integer",
                    "title": "User ID",
                    "description": "The unique identifier of a user",
                    "example": 1234
                },
                "recommendation_page_visit_id": {
                    "type": "integer",
                    "title": "recommendation page visit ID",
                    "description": "The unique identifier of the user presence on the specific             page in the specific time",
                    "example": 1234
                },
                "search_phrase": {
                    "type": "string",
                    "title": "Search phrase",
                    "description": "Search phrase text typed by user in the search panel  in             the context of this recommendation request",
                    "example": "Cloud GPU"
                },
                "filters": {
                    "type": "array",
                    "title": "Filters",
                    "description": "A list of filters chosen by a user in the context of this             recommendation request",
                    "items": {
                        "type": "string",
                        "title": "Filter",
                        "description": "An unambiguous symbol of the filter",
                        "example": "some_filter"
                    }
                }
            },
            "type": "object"
        },
        "User Action": {
            "required": [
                "root",
                "source_page_visit_id",
                "target_page_visit_id",
                "user_action",
                "user_id"
            ],
            "properties": {
                "user_id": {
                    "type": "integer",
                    "title": "User ID",
                    "description": "The unique identifier of a user",
                    "example": 1234
                },
                "source_page_visit_id": {
                    "type": "integer",
                    "title": "Source page visit ID",
                    "description": "The unique identifier of a user presence on the user action             source page in the specific time",
                    "example": 1234
                },
                "target_page_visit_id": {
                    "type": "integer",
                    "title": "Target page visit ID",
                    "description": "The unique identifier of a user presence on the user action             target page in the specific time",
                    "example": 1234
                },
                "user_action": {
                    "type": "string",
                    "title": "User action",
                    "description": "An unambiguous symbol of the user action",
                    "example": "Regular link click"
                },
                "root": {
                    "title": "User journey root",
                    "description": "If this is an action that starts in clicking service             recommended in the recommendation panel or in the regular services list             then it is a root action and this field should be populated",
                    "allOf": [
                        {
                            "$ref": "#/definitions/Root info"
                        }
                    ]
                }
            },
            "type": "object"
        },
        "Root info": {
            "required": [
                "root_type"
            ],
            "properties": {
                "root_type": {
                    "type": "string",
                    "title": "Root Type",
                    "description": "Field informing whether user followed recommended service or             clicked just service in the regular list or in some other place",
                    "example": "recommendation_panel"
                },
                "location": {
                    "type": "string",
                    "title": "Recommendation panel location",
                    "description": "Field used only if the root type is recommendation_panel.             On one page there can be few recommendation panels so this field allows             to distinguish them",
                    "example": "bottom_left_recommendation panel"
                },
                "version": {
                    "type": "string",
                    "title": "Recommendation panel version",
                    "description": "Field used only if the root type is recommendation_panel.             There can be few versions of the same recommendation panel with e.g.             different number of recommended services",
                    "example": "v1"
                },
                "service_id": {
                    "type": "integer",
                    "title": "Service ID",
                    "description": "Field used only if the root type is recommendation_panel.             The unique identifier of a recommended service clicked by the user",
                    "example": 1234
                }
            },
            "type": "object"
        },
        "Database dump": {
            "required": [
                "access_modes",
                "access_types",
                "categories",
                "life_cycle_statuses",
                "platforms",
                "providers",
                "scientific_domains",
                "services",
                "target_users",
                "trls",
                "users"
            ],
            "properties": {
                "services": {
                    "type": "array",
                    "title": "Scientific Services",
                    "description": "List of scientific services",
                    "items": {
                        "title": "Service",
                        "description": "Scientific Service",
                        "allOf": [
                            {
                                "$ref": "#/definitions/Service"
                            }
                        ]
                    }
                },
                "users": {
                    "type": "array",
                    "title": "Users",
                    "description": "List of user",
                    "items": {
                        "title": "User",
                        "description": "User of the Marketplace Portal",
                        "allOf": [
                            {
                                "$ref": "#/definitions/User"
                            }
                        ]
                    }
                },
                "categories": {
                    "type": "array",
                    "title": "Categories",
                    "description": "List of categories",
                    "items": {
                        "title": "Category",
                        "description": "Category",
                        "allOf": [
                            {
                                "$ref": "#/definitions/Category"
                            }
                        ]
                    }
                },
                "providers": {
                    "type": "array",
                    "title": "Providers",
                    "description": "List of providers",
                    "items": {
                        "title": "Provider",
                        "description": "Provider",
                        "allOf": [
                            {
                                "$ref": "#/definitions/Provider"
                            }
                        ]
                    }
                },
                "scientific_domains": {
                    "type": "array",
                    "title": "Scientific Domains",
                    "description": "List of scientific domain",
                    "items": {
                        "title": "Scientific Domain",
                        "description": "Scientific Domain",
                        "allOf": [
                            {
                                "$ref": "#/definitions/Scientific domain"
                            }
                        ]
                    }
                },
                "platforms": {
                    "type": "array",
                    "title": "Platforms",
                    "description": "List of platforms",
                    "items": {
                        "title": "Platform",
                        "description": "Platform",
                        "allOf": [
                            {
                                "$ref": "#/definitions/Platform"
                            }
                        ]
                    }
                },
                "target_users": {
                    "type": "array",
                    "title": "Target users",
                    "description": "List of target users",
                    "items": {
                        "title": "Target user",
                        "description": "Target user",
                        "allOf": [
                            {
                                "$ref": "#/definitions/Target user"
                            }
                        ]
                    }
                },
                "access_modes": {
                    "type": "array",
                    "title": "Access modes",
                    "description": "List of access modes",
                    "items": {
                        "title": "Access mode",
                        "description": "Access mode",
                        "allOf": [
                            {
                                "$ref": "#/definitions/Access mode"
                            }
                        ]
                    }
                },
                "access_types": {
                    "type": "array",
                    "title": "Access types",
                    "description": "List of access types",
                    "items": {
                        "title": "Access type",
                        "description": "Access type",
                        "allOf": [
                            {
                                "$ref": "#/definitions/Access type"
                            }
                        ]
                    }
                },
                "trls": {
                    "type": "array",
                    "title": "TRLs",
                    "description": "List of TRLs",
                    "items": {
                        "title": "TRL",
                        "description": "TRL",
                        "allOf": [
                            {
                                "$ref": "#/definitions/TRL"
                            }
                        ]
                    }
                },
                "life_cycle_statuses": {
                    "type": "array",
                    "title": "TRLs",
                    "description": "List of TRLs",
                    "items": {
                        "title": "TRL",
                        "description": "TRL",
                        "allOf": [
                            {
                                "$ref": "#/definitions/Life cycle status"
                            }
                        ]
                    }
                }
            },
            "type": "object"
        },
        "Service": {
            "required": [
                "access_mode_ids",
                "access_type_ids",
                "category_ids",
                "countries",
                "description",
                "id",
                "life_cycle_status_ids",
                "name",
                "platform_ids",
                "provider_ids",
                "related_service_ids",
                "resource_organisation_id",
                "scientific_domain_ids",
                "tagline",
                "target_user_ids",
                "trl_ids"
            ],
            "properties": {
                "id": {
                    "type": "integer",
                    "title": "Service ID",
                    "description": "The unique identifier of a the service",
                    "example": 1234
                },
                "name": {
                    "type": "string",
                    "title": "Name",
                    "description": "Name of the service",
                    "example": "Cloud GPU"
                },
                "description": {
                    "type": "string",
                    "title": "Description",
                    "description": "description of the service",
                    "example": "Service providing GPU cluster on demand"
                },
                "tagline": {
                    "type": "array",
                    "title": "Tag line",
                    "description": "A list of service's tags",
                    "items": {
                        "type": "string",
                        "title": "Tag",
                        "description": "Tag of the service",
                        "example": "gpu"
                    }
                },
                "countries": {
                    "type": "array",
                    "title": "Countries",
                    "description": "List of countries",
                    "items": {
                        "type": "string",
                        "title": "Country",
                        "description": "Country Alpha-2 code",
                        "example": "PL"
                    }
                },
                "category_ids": {
                    "type": "array",
                    "title": "Categories IDs",
                    "description": "List of categories IDs",
                    "items": {
                        "type": "string",
                        "title": "Category ID",
                        "description": "ID of the category",
                        "example": 1234
                    }
                },
                "provider_ids": {
                    "type": "array",
                    "title": "Providers IDs",
                    "description": "List of providers IDs",
                    "items": {
                        "type": "string",
                        "title": "Provider ID",
                        "description": "ID of the provider",
                        "example": 1234
                    }
                },
                "resource_organisation_id": {
                    "type": "array",
                    "title": "Resource organisations IDs",
                    "description": "List of resource organisations IDs",
                    "items": {
                        "type": "string",
                        "title": "Resource organisation ID",
                        "description": "ID of the resource organisation",
                        "example": 1234
                    }
                },
                "scientific_domain_ids": {
                    "type": "array",
                    "title": "Scientific domains IDs",
                    "description": "List of scientific domains IDs",
                    "items": {
                        "type": "string",
                        "title": "Scientific domain ID",
                        "description": "ID of the scientific domain",
                        "example": 1234
                    }
                },
                "platform_ids": {
                    "type": "array",
                    "title": "Platforms IDs",
                    "description": "List of service's platforms IDs",
                    "items": {
                        "type": "string",
                        "title": "Platform ID",
                        "description": "ID of the service's platform",
                        "example": 1234
                    }
                },
                "target_user_ids": {
                    "type": "array",
                    "title": "Target users IDs",
                    "description": "List of service's target users IDs",
                    "items": {
                        "type": "string",
                        "title": "Target user ID",
                        "description": "ID of the service's target user",
                        "example": 1234
                    }
                },
                "related_service_ids": {
                    "type": "array",
                    "title": "Related services IDs",
                    "description": "List of service's related services IDs",
                    "items": {
                        "type": "string",
                        "title": "Related service ID",
                        "description": "ID of the service's related service",
                        "example": 1234
                    }
                },
                "access_mode_ids": {
                    "type": "array",
                    "title": "Access modes IDs",
                    "description": "List of service's access modes IDs",
                    "items": {
                        "type": "string",
                        "title": "Access mode ID",
                        "description": "ID of the service's access mode",
                        "example": 1234
                    }
                },
                "access_type_ids": {
                    "type": "array",
                    "title": "Access types IDs",
                    "description": "List of service's access types IDs",
                    "items": {
                        "type": "string",
                        "title": "Access type ID",
                        "description": "ID of the service's access type",
                        "example": 1234
                    }
                },
                "trl_ids": {
                    "type": "array",
                    "title": "TRLs IDs",
                    "description": "List of service's TRLs IDs",
                    "items": {
                        "type": "string",
                        "title": "TRL ID",
                        "description": "ID of the service's Technology readiness level",
                        "example": 1234
                    }
                },
                "life_cycle_status_ids": {
                    "type": "array",
                    "title": "Life cycle statuses IDs",
                    "description": "List of service's life cycle statuses IDs",
                    "items": {
                        "type": "string",
                        "title": "Life cycle status ID",
                        "description": "ID of the service's life cycle status",
                        "example": 1234
                    }
                }
            },
            "type": "object"
        },
        "User": {
            "required": [
                "accessed_services_ids",
                "category_ids",
                "id",
                "scientific_domain_ids"
            ],
            "properties": {
                "id": {
                    "type": "integer",
                    "title": "User ID",
                    "description": "The unique identifier of a the user",
                    "example": 1234
                },
                "scientific_domain_ids": {
                    "type": "array",
                    "title": "Scientific domains IDs",
                    "description": "List of scientific domains IDs",
                    "items": {
                        "type": "string",
                        "title": "Scientific domain ID",
                        "description": "ID of the scientific domain",
                        "example": 1234
                    }
                },
                "category_ids": {
                    "type": "array",
                    "title": "Categories IDs",
                    "description": "List of categories IDs",
                    "items": {
                        "type": "string",
                        "title": "Category ID",
                        "description": "ID of the category",
                        "example": 1234
                    }
                },
                "accessed_services_ids": {
                    "type": "array",
                    "title": "Accessed services IDs",
                    "description": "List of accessed services IDs",
                    "items": {
                        "type": "string",
                        "title": "Accessed service ID",
                        "description": "ID of the accessed services",
                        "example": 1234
                    }
                }
            },
            "type": "object"
        },
        "Category": {
            "required": [
                "id",
                "name"
            ],
            "properties": {
                "id": {
                    "type": "integer",
                    "title": "Category ID",
                    "description": "The unique identifier of a the category",
                    "example": 1234
                },
                "name": {
                    "type": "string",
                    "title": "Name",
                    "description": "The name of the category",
                    "example": "Networking"
                }
            },
